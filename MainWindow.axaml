<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="CallSenderApp.MainWindow"
        Title="Call Controller" Width="400" Height="200">
  <StackPanel Margin="10">
    <TextBlock Text="電話番号を入力:" Margin="0,0,0,10" />
    <TextBox Name="PhoneNumberBox" Width="300"/>
    <Button Name="SendButton" Content="発信指示を送信" Margin="0,10,0,0" Width="150"/>
  </StackPanel>
</Window>
